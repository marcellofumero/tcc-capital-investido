{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "quotes":["error","double", {"allowTemplateLiterals":true}],
      "semi":["error","always"],
      "space-before-function-paren":["error","always"]
    }
}
